{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9003",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-api-roles-get": "curl -X GET http://localhost:9003/api/roles",
    "test-api-roles-post": "curl -X POST -H \"Content-Type: application/json\" -d '{\"name\":\"test\",\"code\":\"test\",\"description\":\"test\",\"permissions\":[]}' http://localhost:9003/api/roles",
    "test-api-roles-put": "curl -X PUT -H \"Content-Type: application/json\" -d '{\"name\":\"test2\",\"code\":\"test2\",\"description\":\"test2\",\"permissions\":[]}' http://localhost:9003/api/roles/2",
    "test-api-roles-delete": "curl -X DELETE http://localhost:9003/api/roles/2",
    "test-api-permissions-get": "curl -X GET http://localhost:9003/api/permissions",
    "test-api-permissions-post": "curl -X POST -H \"Content-Type: application/json\" -d '{\"name\":\"test permission\",\"code\":\"test_permission\",\"description\":\"A test permission.\",\"resource\":\"test\",\"action\":\"read\"}' http://localhost:9003/api/permissions",
    "test-api-permissions-put": "curl -X PUT -H \"Content-Type: application/json\" -d '{\"name\":\"updated test permission\",\"description\":\"An updated test permission.\",\"resource\":\"test\",\"action\":\"write\"}' http://localhost:9003/api/permissions/1",
    "test-api-permissions-delete": "curl -X DELETE http://localhost:9003/api/permissions/1",
    "test:e2e:report": "playwright show-report"
  },
