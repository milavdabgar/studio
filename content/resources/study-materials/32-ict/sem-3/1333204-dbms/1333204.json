  "practicalExercises": [
    {
      "srNo": 1,
      "description": "Prepare a report on current database trends, architecture and tools.",
      "unitNo": 1,
      "hours": 2
    },
    {
      "srNo": 2,
      "description": "Draw ER-Diagram for Library Management System and convert it relational schema",
      "unitNo": 2,
      "hours": 2
    },
    {
      "srNo": 3,
      "description": "Implement SQL queries to perform various DDL Commands. (Create minimum 5 tables with different data types and operate upon them)",
      "unitNo": 3,
      "hours": 4
    },
    {
      "srNo": 4,
      "description": "Implement SQL queries to perform various DML Commands. (Insert minimum 10 rows using different insert methods, edit and remove data using update and delete commands) and retrieve data using SELECT command and various SQL operators.",
      "unitNo": 3,
      "hours": 4
    },
    {
      "srNo": 5,
      "description": "Implement SQL queries using Date functions like add-months, months-between, round, nextday, truncate etc",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 6,
      "description": "Implement SQL queries using Numeric functions like abs, ceil, power, mod, round, trunc, sqrt etc. and Character Functions like initcap, lower, upper, ltrim, rtrim, , replace, substring, instr etc.",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 7,
      "description": "Implement SQL queries using Conversion Functions like to-char, todate, to-number and Group functions like Avg, Min, Max, Sum, Count, Decode etc.",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 8,
      "description": "Write SQL query for set operators and join operations.",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 9,
      "description": "Apply the concept of integrity/data constraints while creating/altering a table",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 10,
      "description": "Write SQL queries for CREATE USER, GRANT,REVOKE and DROP USER command.",
      "unitNo": 3,
      "hours": 2
    },
    {
      "srNo": 11,
      "description": "Practices on Normalization – using any database perform various normal forms.",
      "unitNo": 4,
      "hours": 2
    },
    {
      "srNo": 12,
      "description": "Prepare a report on transaction management concepts for concurrent access of database by multiple users.",
      "unitNo": 5,
      "hours": 2
    }
  ],
