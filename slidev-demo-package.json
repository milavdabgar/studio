{
  "name": "slidev-feature-demo",
  "version": "1.0.0",
  "description": "Comprehensive Slidev feature demonstration",
  "type": "module",
  "scripts": {
    "dev": "slidev slidev-demo.md --open",
    "build": "slidev build slidev-demo.md",
    "export": "slidev export slidev-demo.md",
    "export-pdf": "slidev export slidev-demo.md --format pdf",
    "export-png": "slidev export slidev-demo.md --format png",
    "export-md": "slidev export slidev-demo.md --format md",
    "preview": "slidev build slidev-demo.md && slidev --host 0.0.0.0",
    "deploy": "slidev build slidev-demo.md --base /slidev-demo/",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@slidev/cli": "^0.49.0",
    "@slidev/theme-seriph": "^0.27.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.27",
    "@iconify-json/logos": "^1.1.43",
    "@iconify-json/mdi": "^1.1.66",
    "@slidev/types": "^0.49.0",
    "@types/node": "^20.10.0",
    "@unocss/reset": "^0.58.0",
    "eslint": "^8.56.0",
    "playwright-chromium": "^1.40.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "unocss": "^0.58.0"
  },
  "keywords": [
    "slidev",
    "presentation",
    "demo",
    "vue",
    "markdown"
  ],
  "author": "Demo Author",
  "license": "MIT"
}